{
  "kind": "collectionType",
  "collectionName": "objects",
  "info": {
    "singularName": "object",
    "pluralName": "objects",
    "displayName": "Объекты"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "title",
      "required": true
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "meta.meta",
      "repeatable": false
    },
    "object_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::object-category.object-category",
      "inversedBy": "objects"
    },
    "type": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "Отель",
      "enum": [
        "Отель",
        "Курорт",
        "Парк-отель",
        "Гостиница",
        "Ресторан",
        "Пансионат",
        "Горнолыжный комплекс",
        "Горнолыжный курорт",
        "Санаторий",
        "Детский лагерь",
        "Туристический оператор",
        "Другое"
      ]
    },
    "images": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "pdf": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slogan": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "phone": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "website": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "adress": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "location": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "video_url": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "video_poster": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "features": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "features.osobennosti",
      "repeatable": true
    },
    "rooms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::room.room",
      "mappedBy": "object"
    },
    "logo": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "coordinates": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "loyalty_member": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "loyalty_program": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::loyalty-program.loyalty-program",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "loyalty_member"
            },
            true
          ]
        }
      },
      "inversedBy": "objects"
    }
  }
}
