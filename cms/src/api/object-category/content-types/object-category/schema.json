{
  "kind": "collectionType",
  "collectionName": "object_categories",
  "info": {
    "singularName": "object-category",
    "pluralName": "object-categories",
    "displayName": "Категории объектов"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "icon": {
      "type": "string"
    },
    "objects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::object.object",
      "mappedBy": "object_category"
    }
  }
}
